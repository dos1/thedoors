pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
initial_delay = 15
time_to_spawn = 100
kids = {}

function _init()
  left = 0
  right = 0
  delay = initial_delay
  timer = delay
  counter = 0
  kids={{x=50, y=128, delay=3, state=false, althair=false}}
end

function _updategame()
  time_to_spawn -= 1
  
  if (time_to_spawn == 0) then
    -- 32 to 84
    add(kids, {x=flr(rnd(84-32+1))+32, y=128, delay=3, state=false, althair=flr(rnd(2))})
    time_to_spawn = 100
  end
  
  for kid in all(kids) do
    kid.delay -= 1
    if (kid.delay == 0) then
      kid.delay = 3
      kid.state = not kid.state
      kid.y -= 1
    end
  end

  if (btnp(0)) then
    left += 1
  end
  if (btnp(1)) then
    right += 1
  end
  timer -= 1
  if (timer < 0) then
    timer = delay
    left -= 1
    right -= 1
    counter += 1
    if (counter%60 == 0) then
      delay -= 1
    end
  end
  if (left < 0) then
    left = 0
  end
  if (right < 0) then
    right =0
  end  
  if (left > 32) then
    left = 32
  end
  if (right > 32) then
    right = 32
  end
end

function _updatemenu()
  if (btnp(0) or btnp(1)) then
    _update = _updategame
    _draw = _drawgame
    music(1)
  end
end

function print_centered(str, x, y, col)
  print(str, x - (#str * 2), y, col) 
end


function _drawmenu()
  cls()

  rectfill(0, 0, 127, 111, 2)
  rectfill(0, 112, 127, 127, 13)

  rectfill(8*4-1, 8*2,
       8*8-1-1-left, 8*14-1,
       12)
  rectfill(8*8+1+right, 8*2,
       8*12, 8*14-1,
       12)
  line(8*8-1-left, 8*2,
       8*8-1-left, 8*14-1,
       1)
  line(8*8+right, 8*2,
       8*8+right, 8*14-1,
       1)
  line(8*4, 8*2+4,
       8*8-1-1-left, 8*2+4,
       1)
  line(8*8+1+right, 8*2+4,
       8*12-1, 8*2+4,
       1)
  
  map(0,0,0,0,16,16)
  
  print_centered("THE DOORS", 64, 13, 1)
end  

_update = _updatemenu
_draw = _drawmenu

function _drawgame()
  cls()

  rectfill(0, 0, 127, 111, 2)
  rectfill(0, 112, 127, 127, 13)
  rectfill(30, 19, 97, 111, 13)

  for kid in all(kids) do
    if (kid.y <= 100) then
      local xxx = 9*8 + 2
      if (kid.althair == 1) then
        xxx += 16
      end
      sspr(xxx, 0, 12, 8, kid.x, kid.y)
      xxx = 9*8 + 2
      if (kid.state) then
        xxx += 16
      end
    
      sspr(xxx, 8, 12, 8, kid.x, kid.y+8)
    end
  end
  
  rectfill(0, 0, 127, 18, 2)

  rectfill(8*4-1, 8*2,
       8*8-1-1-left, 8*14-1,
       12)
  rectfill(8*8+1+right, 8*2,
       8*12, 8*14-1,
       12)
  line(8*8-1-left, 8*2,
       8*8-1-left, 8*14-1,
       1)
  line(8*8+right, 8*2,
       8*8+right, 8*14-1,
       1)
  line(8*4, 8*2+4,
       8*8-1-1-left, 8*2+4,
       1)
  line(8*8+1+right, 8*2+4,
       8*12-1, 8*2+4,
       1)
  
  map(0,0,0,0,16,16)
  print_centered("THE DOORS", 64, 13, 1)

  for kid in all(kids) do
    if (kid.y > 100) then
      local xxx = 9*8 + 2
      if (kid.althair == 1) then
        xxx += 16
      end
      sspr(xxx, 0, 12, 8, kid.x, kid.y)
      xxx = 9*8 + 2
      if (kids[1].state) then
        xxx += 16
      end
    
      sspr(xxx, 8, 12, 8, kid.x, kid.y+8)
    end
  end
end
